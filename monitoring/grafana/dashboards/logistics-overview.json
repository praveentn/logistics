{
  "dashboard": {
    "title": "Logistics Overview",
    "tags": ["logistics", "microservices"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "type": "graph",
        "title": "HTTP Request Rate",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Requests/sec"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "show": true
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "nullPointMode": "null",
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": false,
          "current": true,
          "max": true,
          "min": false,
          "rightSide": false,
          "values": true
        }
      },
      {
        "id": 2,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "type": "graph",
        "title": "HTTP Request Duration (p95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{endpoint}} (p95)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{endpoint}} (p50)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "show": true
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "nullPointMode": "null"
      },
      {
        "id": 3,
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 8
        },
        "type": "stat",
        "title": "Total Orders Created",
        "targets": [
          {
            "expr": "orders_created_total",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 100,
                  "color": "yellow"
                },
                {
                  "value": 1000,
                  "color": "red"
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 8
        },
        "type": "stat",
        "title": "Service Status",
        "targets": [
          {
            "expr": "service_up",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "1": {
                    "text": "UP",
                    "color": "green"
                  },
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  }
                }
              }
            ],
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 8
        },
        "type": "graph",
        "title": "Orders by Status",
        "targets": [
          {
            "expr": "orders_by_status",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Count"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "show": true
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "stack": true
      },
      {
        "id": 6,
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 14
        },
        "type": "table",
        "title": "HTTP Requests Summary",
        "targets": [
          {
            "expr": "sum by (method, endpoint, status) (http_requests_total)",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {},
              "renameByName": {
                "method": "Method",
                "endpoint": "Endpoint",
                "status": "Status",
                "Value": "Total Requests"
              }
            }
          }
        ]
      }
    ],
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  },
  "overwrite": true
}
